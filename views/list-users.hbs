<style>
  #users li, div {
    border: 1px solid black;
  }
</style>

<ul id="users">
  
</ul>
<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.18.0/axios.js"></script>
<script>

  axios.get("http://localhost:3000/users/100").then((usersPayload) => {
    let output4 = usersPayload.data.users.map((user) => {
      const domEl = document.createElement("li")
      domEl.innerHTML = user.name

      return domEl
    })

    output4.forEach((DOMEl) => {
      document.querySelector("#users").appendChild(DOMEl)
    })
  })
</script>